<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--原始版本的servlet加mapping映射-->
    <servlet>
        <servlet-name>MyFirstServletName</servlet-name>
        <servlet-class>com.skyline.MyFirstServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyFirstServletName</servlet-name>
        <url-pattern>/helloTwo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MyFirstServletOne</servlet-name>
        <servlet-class>com.skyline.MyFirstServletOne</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyFirstServletOne</servlet-name>
        <url-pattern>/helloOne</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MySelf</servlet-name>
        <servlet-class>com.skyline.MySelf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MySelf</servlet-name>
        <url-pattern>/mySelf</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MyHello</servlet-name>
        <servlet-class>com.skyline.MyHelloWorld</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyHello</servlet-name>
        <url-pattern>/myHello</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MyForm</servlet-name>
        <servlet-class>com.skyline.MyForm</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyForm</servlet-name>
        <url-pattern>/java_web01_war_exploded/MyForm</url-pattern>
    </servlet-mapping>

    <!--    MyPostForm-->
    <servlet>
        <servlet-name>MyPostForm</servlet-name>
        <servlet-class>com.skyline.MyPostForm</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyPostForm</servlet-name>
        <url-pattern>/java_web01_war_exploded/MyPostForm</url-pattern>
    </servlet-mapping>
    <!--    MyPostForm-->
    <!--  Refresh  -->
    <servlet>
        <servlet-name>Refresh</servlet-name>
        <servlet-class>com.skyline.Refresh</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Refresh</servlet-name>
        <url-pattern>/refresh</url-pattern>
    </servlet-mapping>
    <!--  Refresh  -->

    <!--    DisplayHeader-->
    <servlet>
        <!-- 类名 -->
        <servlet-name>DisplayHeader</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.runoob.test.DisplayHeader</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayHeader</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/java_web01_war_exploded/DisplayHeader</url-pattern>
    </servlet-mapping>
    <!--    DisplayHeader-->

    <!--    DatabaseAccess-->
    <servlet>
        <servlet-name>DatabaseAccess</servlet-name>
        <servlet-class>com.skyline.DatabaseAccess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DatabaseAccess</servlet-name>
        <url-pattern>/java_web01_war_exploded/DatabaseAccess</url-pattern>
    </servlet-mapping>
    <!--    DatabaseAccess-->

    <!--    DatabaseAccess-->
    <servlet>
        <servlet-name>MyDatabase</servlet-name>
        <servlet-class>com.skyline.MyDatabase</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyDatabase</servlet-name>
        <url-pattern>/java_web01_war_exploded/MyDatabase</url-pattern>
    </servlet-mapping>
    <!--    DatabaseAccess-->

    <!--    DatabaseAccess-->
    <servlet>
        <servlet-name>getuser</servlet-name>
        <servlet-class>com.user.GetUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getuser</servlet-name>
        <url-pattern>/java_web01_war_exploded/getuser</url-pattern>
    </servlet-mapping>
    <!--    DatabaseAccess-->

    <!--    addUser-->
    <servlet>
        <servlet-name>addUser</servlet-name>
        <servlet-class>com.user.AddUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addUser</servlet-name>
        <url-pattern>/java_web01_war_exploded/addUser</url-pattern>
    </servlet-mapping>
    <!--    addUser-->

    <!-- editorUser -->
    <servlet>
        <servlet-name>editUser</servlet-name>
        <servlet-class>com.user.EdirtorUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editUser</servlet-name>
        <url-pattern>/java_web01_war_exploded/editUser</url-pattern>
    </servlet-mapping>
    <!-- editorUser -->

    <!-- deleteUser -->
    <servlet>
        <servlet-name>deleteUser</servlet-name>
        <servlet-class>com.user.deleteUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deleteUser</servlet-name>
        <url-pattern>/java_web01_war_exploded/deleteUser</url-pattern>
    </servlet-mapping>
    <!-- deleteUser -->

    <filter>
        <filter-name>addUserFilter</filter-name>
        <filter-class>com.filter.addUserFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>addUserFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <!--    getUser查询的过滤-->
    <filter>
        <filter-name>getUserFilter</filter-name>
        <filter-class>com.filter.GetUserFilter</filter-class>

    </filter>
    <filter-mapping>
        <filter-name>getUserFilter</filter-name>
        <url-pattern>/java_web01_war_exploded/*</url-pattern>
        <servlet-name>getuser</servlet-name>
    </filter-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.jpg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.html</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.png</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.js</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.css</url-pattern>
    </servlet-mapping>


    <!--   Servlet 过滤器 过滤器-->
    <filter>
        <filter-name>LogFilter</filter-name>
        <filter-class>com.runoob.test.LogFilter</filter-class>
        <init-param>
            <param-name>Site</param-name>
            <param-value>菜鸟教程</param-value>
        </init-param>
        <init-param>
            <param-name>name</param-name>
            <param-value>测试</param-value>
        </init-param>
        <init-param>
            <param-name>url</param-name>
            <param-value>www.huamei.com</param-value>
        </init-param>
    </filter>

    <filter-mapping>
        <filter-name>LogFilter</filter-name>
        <url-pattern>/skyline/*</url-pattern>
    </filter-mapping>

    <!--   Servlet 过滤器 过滤器-->
</web-app>